<!DOCTYPE html>
<html lang="en">
<head>
    <style> #homenav { color: #ffffff !important; font-weight: bolder; } </style>
    <%- include('./partials/head'); %>
</head>
<body class="">

    <!-- Header Start -->
        <%- include('./partials/header-admin'); %>
    <!-- Header End -->

    <!-- Carousel Start -->
    <div id="carouselExampleIndicators" class="carousel slide">
        <div class="carousel-indicators">
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
            <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="3" aria-label="Slide 4"></button>
        </div>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="/images/carousel-1.jpg" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="/images/carousel-2.jpg" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="/images/carousel-3.jpg" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="/images/carousel-4.jpg" class="d-block w-100" alt="...">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
    <!-- Carousel End -->

    <!-- Feature Start -->
    <div class="text-bg-light py-3">
        <div class="container marketing mt-0 py-5">
            <div class="row featurette">
                <div class="col-md-7">
                    <h2 class="featurette-heading fw-normal lh-1">Popular Facilities</h2>
                    <p class="lead pt-1">
                        <img src="/icons/swimming.svg" alt="Swimming" width="20px"> Swimming Pool.<br>
                        <img src="/icons/room-service.svg" alt="Room Service" width="20px"> Room Service.<br>
                        <img src="/icons/volume-mute.svg" alt="Soundproofed Rooms" width="20px"> Soundproofed Rooms.<br>
                        <img src="/icons/restaurant.svg" alt="Restaurant" width="20px"> Restaurant.<br>
                        <img src="/icons/24-hours.svg" alt="24/7 Front Desk" width="20px"> 24/7 Front Desk.<br>
                        <img src="/icons/air-conditioner.svg" alt="Air Conditioner" width="20px"> Air Conditioner.<br>
                        <img src="/icons/bell.svg" alt="Personal Assistant Service" width="20px"> Personal Assistant Service.<br>
                        <img src="/icons/wifi.svg" alt="Free Wi-Fi" width="20px"> Free Wi-Fi Internet Access.<br>
                    </p>
                </div>
                <div class="col-md-5">
                    <img src="/images/news1.jpg" class="bd-placeholder-img bd-placeholder-img-lg featurette-image img-fluid mx-auto" width="500" height="500" alt="The Paradise Resort" title="Popular Facilities">
                </div>
            </div>
        
            <hr class="featurette-divider py-2">
        
            <div class="row featurette">
                <div class="col-md-7 order-md-2">
                    <h2 class="featurette-heading fw-normal lh-1">Terms and Conditions</h2>
                    <p class="lead pt-1">
                        <img src="/icons/check-in.svg" alt="Check-in" width="20px"> Check-in : From 14:00<br>
                        <img src="/icons/check-out.svg" alt="Check-out" width="20px"> Check-out : Until 12:00<br>
                        <img src="/icons/info-circle.svg" alt="Cancel-Pre" width="20px"> Cancellation/Prepayment : Depending on the type of accommodation.<br>
                        <img src="/icons/age.svg" alt="Age" width="20px"> Age : No Age Restrictions.<br>
                        <img src="/icons/pets.svg" alt="Pets" width="20px"> Pets : Not Allowed.<br>
                        <img src="/icons/credit-card.svg" alt="Payment" width="20px"> Payment Methods : Mastercard, Visa, Cash.<br>
                    </p>
                </div>
                <div class="col-md-5">
                    <img src="/images/news2.jpg" class="bd-placeholder-img bd-placeholder-img-lg featurette-image img-fluid mx-auto" width="500" height="500" alt="The Paradise Resort" title="Terms and Conditions">
                </div>
            </div>
        
            <hr class="featurette-divider py-2">
        
            <div class="row featurette">
                <div class="col-md-7">
                    <h2 class="featurette-heading fw-normal lh-1">Top Attractions</h2>
                    <p class="lead pt-1">
                        <img src="/icons/1-circle.svg" alt="Check-in" width="20px"> Kathu Mining Museum.<br>
                        <img src="/icons/2-circle.svg" alt="Check-out" width="20px"> Chalong Pier.<br>
                        <img src="/icons/3-circle.svg" alt="Cancel-Pre" width="20px"> Two Heroines Monument.<br>
                        <img src="/icons/4-circle.svg" alt="Age" width="20px"> Phuket Seashell Museum.<br>
                        <img src="/icons/5-circle.svg" alt="Pets" width="20px"> Kata View Point.<br>
                        <img src="/icons/6-circle.svg" alt="Payment" width="20px"> Windmill Viewpoint.<br>
                        <img src="/icons/7-circle.svg" alt="Pets" width="20px"> Khao Phra Thaeo National Park.<br>
                        <img src="/icons/8-circle.svg" alt="Payment" width="20px"> Promthep Cape.<br>
                    </p>
                </div>
                <div class="col-md-5">
                    <img src="/images/news3.jpg" class="bd-placeholder-img bd-placeholder-img-lg featurette-image img-fluid mx-auto" width="500" height="500" alt="The Paradise Resort" title="Top Attractions">
                </div>
            </div>
        </div>
    </div>
    <!-- Feature End -->

    <!-- Footer Start -->
        <%- include('./partials/footer-admin') %>
    <!-- Footer End -->


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
    <script src="/js/app.js"></script>
</body>
</html>